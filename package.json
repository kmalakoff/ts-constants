{
  "name": "ts-constants",
  "version": "1.0.3",
  "description": "typescript constants extracted",
  "keywords": [
    "typescript",
    "constants",
    "ScriptTarget",
    "ModuleKind",
    "JsxEmit"
  ],
  "homepage": "https://github.com/kmalakoff/ts-constants#readme",
  "bugs": {
    "url": "https://github.com/kmalakoff/ts-constants/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmalakoff/ts-constants.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff",
  "main": "dist/cjs/index.cjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "clean": "node -e \"const dir = require('path').join(require('homedir-polyfill')(), '.im'); try {require('fs').rmdirSync(dir, {recursive: true})} catch(err) {}\"",
    "deploy": "tsds deploy",
    "docs": "tsds docs",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.5",
    "assert": "^2.1.0"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs"
    ]
  }
}
